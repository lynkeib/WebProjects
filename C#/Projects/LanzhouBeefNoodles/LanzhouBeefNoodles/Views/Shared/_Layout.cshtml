<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Using Template @ViewBag.Title</title>
    <link type="text/css" rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.css"/>
    <link type="text/css" rel="stylesheet" href="~/content/site.css" />
</head>

<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-controller="Home" asp-action="Index" class="navbar-brand">Homepage</a></li>
                    <li><a asp-controller="Feedback" asp-action="Index">Feedback</a></li>
                    <partial name="_LoginPartial">
            </div>
        </div>
    </nav>
    <div class="container body-content">
        <div class="row">
            @RenderBody()
        </div>
    </div>
 
</body>
</html>
