<%- include("partials/header.ejs") %>

<h1>
	This is the index page
</h1>

<% blogs.forEach(function(blog){ %>
	<h2>
		<%= blog["title"] %>
	</h2>
	<img src="<%= blog["image"] %>">
	<span><%= blog["create"] %></span>
	<p>
		<%= blog["body"] %>
	</p>
	<a href="/blogs/<%= blog["_id"] %>">READ MORE</a>
<% }) %>

<%- include("partials/footer.ejs") %>